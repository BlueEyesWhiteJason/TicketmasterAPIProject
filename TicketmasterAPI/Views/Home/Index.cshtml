@model List<Event>


<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                url
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a href=@item.Url>Event Link</a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GenreName)
            </td>
            <td>
                @Html.ActionLink("Favorite", "addFavorite", "FavEvents",
               new { name = item.Name, url=item.Url, city=item.City,
                   state =item.State, date=item.Date, genreName=item.GenreName }, null)
            </td>

        </tr>
        }
    </tbody>
</table>
