}
pooperdoo
<h1>shoot</h1>
@ViewBag.count

@foreach (var item in Model)
{
    <div>
        @Html.DisplayFor(modelItem => item.Name)
    </div>
}

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                url
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
